{
    "name": "asm89/twig-lint",
    "description": "Standalone twig linter.",
    "keywords": ["twig", "lint"],
    "homepage": "https://github.com/asm89/twig-lint",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander",
            "email": "iam.asm89@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^2.1 || ^3.0 ||^4.0",
        "symfony/finder": "^2.1 || ^3.0 ||^4.0",
        "twig/twig": "^1.16.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36 || ^5.5 || ^6.2"
    },
    "autoload": {
        "psr-0": { "Asm89\\Twig\\Lint\\": "src/" }
    },
    "bin": ["bin/twig-lint"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
